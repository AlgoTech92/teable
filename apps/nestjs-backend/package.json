{
  "name": "@teable-group/backend",
  "version": "3.55.0",
  "license": "AGPL-3.0",
  "private": true,
  "main": "dist/bootstrap.js",
  "homepage": "https://github.com/teable-group/teable",
  "repository": {
    "type": "git",
    "url": "https://github.com/teable-group/teable",
    "directory": "apps/nestjs-backend"
  },
  "author": {
    "name": "tea artist",
    "url": "https://github.com/tea-artist"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "clean": "rimraf --no-glob ./.next ./out ./coverage ./tsconfig.tsbuildinfo ./main ./dist",
    "build": "yarn clean && tsc --build",
    "dev": "yarn clean && nest start --watch --tsc",
    "start": "NODE_ENV=production node ./dist",
    "?share-static-symlink": "echo 'Use this command to link assets... from shared static folder'",
    "share-static-symlink": "rimraf ./public/shared-assets && symlink-dir ../../static/assets ./public/shared-assets",
    "?share-static-hardlink": "echo 'Use this command to link assets... from shared static folder'",
    "share-static-hardlink": "rimraf ./public/shared-assets && syncdir ../../static/assets ./public/shared-assets --copy",
    "check-dist": "es-check -v",
    "check-size": "size-limit --highlight-less",
    "test": "yarn test-unit",
    "test-unit": "vitest run",
    "test-unit-jest": "jest --config ./jest.config.js",
    "test-unit-watch": "vitest watch --ui",
    "test-e2e": "cross-env E2E_WEBSERVER_MODE=START playwright test",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/nextjs-app.eslintcache",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "flamegraph-home": "npx 0x --output-dir './.debug/flamegraph/{pid}.0x' --on-port 'autocannon http://localhost:$PORT --duration 20' -- node ../../node_modules/.bin/next start"
  },
  "devDependencies": {
    "@nestjs/testing": "9.2.0",
    "@types/better-sqlite3": "7.6.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.0",
    "cross-env": "7.0.3",
    "dotenv-flow": "3.2.0",
    "dotenv-flow-cli": "1.0.0",
    "es-check": "7.0.1",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "get-tsconfig": "4.2.0",
    "jest": "29.2.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "symlink-dir": "5.0.1",
    "sync-directory": "5.1.9",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "ts-patch": "2.0.2",
    "typescript": "4.8.4",
    "typescript-transform-paths": "3.4.4"
  },
  "dependencies": {
    "@babel/core": "7.19.6",
    "@belgattitude/http-exception": "1.4.0",
    "@nestjs/common": "9.2.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.0",
    "@nestjs/platform-express": "9.2.0",
    "@nestjs/swagger": "6.1.3",
    "@teable-group/common-i18n": "workspace:^",
    "@teable-group/db-main-prisma": "workspace:^",
    "@teable-group/sdk": "workspace:^",
    "@teable-group/ts-utils": "workspace:^",
    "axios": "1.1.3",
    "better-sqlite3": "8.0.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "is-port-reachable": "3.1.0",
    "lodash": "4.17.21",
    "next": "13.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.7",
    "zod": "3.19.1"
  }
}
