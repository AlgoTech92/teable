    "@teable/openapi": "workspace:^",
    "@teable/sdk": "workspace:^",
    "@teable/ui-lib": "workspace:^",
    "i18next": "23.16.0",
    "i18next-resources-to-backend": "1.2.1",
    "lucide-react": "0.453.0",
    "next": "14.2.15",
    "next-secure-headers": "2.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.0",
    "react-i18next": "15.0.3",
    "recharts": "2.13.0",
